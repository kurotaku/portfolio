<!DOCTYPE html>
<html>
  <head>
    <title>App</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_pack_tag 'application' %>
  </head>

  <body class="body">
    <div class="main-wrap">
      <%= render 'layouts/main_menu' %>
      <div class="main-contents">
        <%= render 'layouts/flash_messages' %>
        <%= yield %>
      </div><!-- .main-contents -->
    </div><!-- .main-wrap -->
  </body>
</html>
